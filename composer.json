{
	"name": "weareknit/silverstripe-admin-google-map",
	"description": "Display a Google Map in the Silverstripe CMS admin to set latitude and longitude data.",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"google",
		"map",
		"cms",
		"admin",
		"latitude",
		"longitude"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Matt Kibbler",
			"email": "matt.kibbler@weareknit.co.uk"
		}
	],
	"require": {
		"silverstripe/framework": "^4",
		"silverstripe/cms": "^4"
	},
	"extra": {
		"expose": [
			"client"
		],
		"screenshots": [
			"docs/screenshot.png"
		]
	},
	"replace": {
		"weareknit/silverstripe-admin-google-map": "self.version"
	},
	"autoload": {
		"psr-4": {
			"WeAreKnit\\SSAdminGoogleMap\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability" : "dev"
}
